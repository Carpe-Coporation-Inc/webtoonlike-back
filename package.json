{
  "name": "kenaz-ip-back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm tsc && pnpm tsc-alias",
    "apis": "nodemon --exec ts-node --files -r tsconfig-paths/register ./src/main.ts",
    "serve-apis": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "mcreate": "knex --knexfile ./src/knexfile.ts  migrate:make -x ts",
    "mlatest": "knex --knexfile ./src/knexfile.ts migrate:latest",
    "mreset": "knex --knexfile ./src/knexfile.ts migrate:rollback --all",
    "mup": "knex --knexfile ./src/knexfile.ts migrate:up",
    "mdown": "knex --knexfile ./src/knexfile.ts migrate:down",
    "mlist": "knex --knexfile ./src/knexfile.ts migrate:list",
    "gentype": "env TYPE_GEN=\"ts\" ts-node -T --files -r tsconfig-paths/register src/gentype.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "glob": "^10.3.12",
    "jsonwebtoken": "^9.0.2",
    "kenaz-ip-back": "link:",
    "knex": "^3.1.0",
    "mime-types": "^2.1.35",
    "nestjs-zod": "^3.0.0",
    "nodemon": "^3.1.0",
    "pdf-puppeteer": "^1.1.11",
    "pg": "^8.11.4",
    "puppeteer": "^22.6.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/html-pdf": "^3.0.3",
    "@types/html-pdf-node": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.12.2",
    "@types/pg": "^8.11.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3",
    "zod-to-ts": "^1.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
